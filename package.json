{
  "name": "@subsquid/cli",
  "description": "squid cli tool",
  "version": "2.8.6",
  "license": "GPL-3.0-or-later",
  "repository": "git@github.com:subsquid/squid-cli.git",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "sqd": "./bin/run.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "pkg": {
    "scripts": "./lib/**/*.js",
    "assets": "./node_modules/**/*"
  },
  "oclif": {
    "commands": "./lib/commands",
    "default": "default",
    "helpClass": "./lib/help",
    "bin": "sqd",
    "repositoryPrefix": "<%- repo %>/tree/master/<%- commandPath %>",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-autocomplete"
    ],
    "topics": {
      "squid": {
        "description": "Manage squid deployments to the Subsquid Cloud service."
      },
      "secrets": {
        "description": "Manage account secrets. The secrets are exposed as environment variables, and are accessible to all the squids deployed by the current Cloud account."
      }
    },
    "hooks": {
      "command_not_found": "./lib/hooks/command_not_found"
    }
  },
  "homepage": "https://www.subsquid.io/",
  "scripts": {
    "postinstall": "node ./bin/run.js autocomplete --refresh-cache",
    "build": "rm -rf lib && tsc",
    "dev": "./bin/dev",
    "sqd": "./bin/run.js",
    "bl": "node ./lib/blessed.js",
    "lint": "eslint --fix src/**/*",
    "test:unit": "NODE_ENV=test jest --bail --testRegex=.unit.spec.ts$",
    "tsc": "tsc --noEmit",
    "pkg:build": "./bin/pkg-build.sh",
    "pkg:compress": "./bin/pkg-compress.sh",
    "upg": "npx npm-check-updates -u -i"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node"
  },
  "dependencies": {
    "@oclif/core": "^3.19.1",
    "@oclif/plugin-autocomplete": "^3.0.8",
    "@subsquid/commands": "^2.3.1",
    "@subsquid/manifest": "^0.0.1-beta.17",
    "@subsquid/manifest-expr": "^0.0.1",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/lodash": "^4.14.202",
    "@types/targz": "^1.0.4",
    "async-retry": "^1.3.3",
    "blessed-contrib": "^4.11.0",
    "chalk": "^4.1.2",
    "cli-select": "^1.1.2",
    "cross-spawn": "^7.0.3",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.2",
    "fast-levenshtein": "^3.0.0",
    "figlet": "^1.7.0",
    "form-data": "^4.0.0",
    "inquirer": "^8.2.5",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "neo-blessed": "^0.2.0",
    "node-fetch": "^2.6.7",
    "pretty-bytes": "^5.6.0",
    "qs": "^6.11.2",
    "reblessed": "^0.2.1",
    "simple-git": "^3.22.0",
    "split2": "^4.2.0",
    "targz": "^1.0.1",
    "tree-kill": "^1.2.2",
    "unique-names-generator": "^4.7.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/help": "^1.0.4",
    "@types/async-retry": "^1.4.8",
    "@types/blessed": "^0.1.25",
    "@types/cross-spawn": "^6.0.6",
    "@types/figlet": "^1.5.8",
    "@types/inquirer": "^8.2.3",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.11.17",
    "@types/node-fetch": "^2.6.11",
    "@types/qs": "^6.9.11",
    "@types/split2": "^3.2.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/eslint-plugin-tslint": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^4.1.0",
    "jest": "^29.7.0",
    "pkg": "^5.8.1",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "~5.3.3"
  }
}
